<div id="main">
    <h1> Login </h1>

    <div id="frm">
        <%= form_tag('/signin') do %>
            <%= label_tag :email %>
            <%= text_field_tag :email %>
            <%= label_tag :password %>
            <%= password_field_tag :password %>
            <%= submit_tag "Sign In" %>
        <% end %>
    </div>
</div>
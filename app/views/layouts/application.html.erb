<!DOCTYPE html>
<html>
  <head>
    <title>Decider</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if flash[:message] %>
      <%= flash[:message] %>
    <% end %>

    <div id="navbar">
      <ul>
        <li><link_to "Home", root_path></li>
      </ul>
    <div>
    <%= yield %>
  </body>
</html>
